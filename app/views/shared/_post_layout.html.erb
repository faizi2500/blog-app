<div class="post-section">
  <a href='<%= request.path %>/<%=post.id%>'><p>Post #<%= index + 1 %></p></a>
  <p class="post-text"><%=post.text%> </p>
  <div class="reaction">
    <p> Likes: <%= Like.where('post_id = ?', post.id).count %></p>
    <p> Comments:<%= Comment.where('post_id = ?', post.id).count %></p>
  </div>
</div>
